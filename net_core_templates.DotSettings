	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=B5217CCB71C9C14A99CFD90E5C6C086C/Text/@EntryValue">public Task&lt;HealthCheckResult&gt; CheckHealthAsync(HealthCheckContext context,&#xD;
            CancellationToken cancellationToken = new CancellationToken())&#xD;
        {&#xD;
            try&#xD;
            {&#xD;
                using (var connection = new SqlConnection(_connectionString))&#xD;
                {&#xD;
                    connection.Open();&#xD;
                }&#xD;
            }&#xD;
            catch (SqlException)&#xD;
            {&#xD;
                return Task.FromResult(HealthCheckResult.Unhealthy());&#xD;
            }&#xD;
&#xD;
            return Task.FromResult(HealthCheckResult.Healthy());&#xD;
        }</s:String>
